<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE language SYSTEM "language.dtd">
<language name="TJA" section="Other" extensions="*.tja" mimetype="" version="1" kateversion="5.0" author="Katie Frogs" license="PD">
	<highlighting>
		<list name="MetadataFloat">
			<item>BPM</item>
			<item>OFFSET</item>
			<item>DEMOSTART</item>
			<item>SONGVOL</item>
			<item>SEVOL</item>
			<item>HEADSCROLL</item>
			<item>MOVIEOFFSET</item>
		</list>
		<list name="MetadataIntString">
