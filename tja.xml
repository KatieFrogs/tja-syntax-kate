			<item>TOTAL</item>
			<item>HIDDENBRANCH</item>
		</list>
		<list name="CourseString">
			<item>GAUGEINCR</item>
			<item>NOTESDESIGNER0</item>
			<item>NOTESDESIGNER1</item>
			<item>NOTESDESIGNER2</item>
			<item>NOTESDESIGNER3</item>
			<item>NOTESDESIGNER4</item>
			<item>NOTESDESIGNER5</item>
